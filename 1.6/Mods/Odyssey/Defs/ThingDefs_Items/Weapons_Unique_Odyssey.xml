<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="VWE_Crossbow_Base">
		<defName>VWE_Bow_Crossbow_Unique</defName>
		<label>unique crossbow</label>
		<description>A crossbow with customized parts.</description>
		<graphicData>
			<texPath>Things/Item/Equipment/WeaponRanged/UniqueWeapons/UniqueCrossbow</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<!-- Can't inherit comps because we need to override CompEquippable with CompEquippableAbility -->
		<!-- If changes are made to the comps of BaseWeaponNeolithic or parents then they will need to be replicated here -->
		<comps Inherit="False">
			<li Class="CompProperties_EquippableAbilityReloadable"/>
			<!-- Props are populated by traits -->
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Styleable"/>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="CompProperties_UniqueWeapon">
				<weaponCategories>
					<li>Ranged</li>
					<li>Bow</li>
				</weaponCategories>
				<namerLabels>
					<li>crossbow</li>
				</namerLabels>
			</li>
		</comps>
		<generateAllowChance>0</generateAllowChance>
		<possessionCount>0</possessionCount>
		<tradeability>Sellable</tradeability>
		<thingSetMakerTags Inherit="False">
			<li>RewardStandardHighFreq</li>
			<li>UniqueWeapon</li>
		</thingSetMakerTags>
		<recipeMaker Inherit="False" IsNull="True"/>
		<thingCategories Inherit="False">
			<li>WeaponsUnique</li>
		</thingCategories>
		<descriptionHyperlinks>
			<ThingDef>VWE_Bow_Crossbow</ThingDef>
		</descriptionHyperlinks>
	</ThingDef>
